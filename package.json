  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=production nyc mocha --compilers js:@babel/register --inspect test/**/*.js --exit -r dotenv/config",
    "dev": "nodemon --exec npx babel-node --inspect  ./src/function",
    "clean-db": "nodemon --watch ./graph/src --ext es6,mjs,js,cjs,json --exec \"npx babel-node --inspect ./utils/clean-tables.js\"",
    "setup-db": "nodemon --watch ./graph/src --ext es6,mjs,js,cjs,json --exec \"npx babel-node --inspect ./utils/create-tables.js\"",
    "build": "npx babel ./src --out-dir dist --source-maps && node copy-package.js",
    "deploy": "yarn build && cd dist &&  gcloud functions deploy shuleplus-server --runtime=nodejs22 --trigger-http",
    "test-deployment": "yarn build && node dist/function",
    "start": "node dist/function",
    "debug": "babel-node debug ./src",
    "deploy:pm2": "yarn build && pm2 start dist/function.js --name shuleplus-server",
    "update:restart": "git pull && yarn install && yarn build && pm2 restart shuleplus-server"
  },
